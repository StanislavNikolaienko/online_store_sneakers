[flake8]
ignore = E203, E266, E501, W503, W504, F403, F401,F405
max-line-length = 79
max-complexity = 18
exclude=*/migrations/*
per-file-ignores =
    ./random_file.py:E501
    ./settings/local.py:F405,F403
    ./settings/prod.py:F405,F403,F401
    ./settings/staging.py:F405,F403
    ./accounts/apps.py:F40
